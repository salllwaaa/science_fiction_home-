#version 330 core

// Ouput data
out vec3 color;
in vec3 pixelcolor;
in vec2 Tex;
uniform sampler2D myTextureSampler;
void main()
{
    if(pixelcolor.x==0 && pixelcolor.y==0 && pixelcolor.z==0){

	    color = texture(myTextureSampler,Tex).rgb;
	}
	else{
      color = pixelcolor;
	}
}
